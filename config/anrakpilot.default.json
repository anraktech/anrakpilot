{
  "agents": {
    "defaults": {
      "model": {
        "primary": "openrouter/openrouter/free",
        "fallbacks": ["openrouter/moonshotai/kimi-k2-thinking", "openrouter/openrouter/auto"]
      },
      "models": {
        "openrouter/openrouter/free": {
          "alias": "free",
          "streaming": true
        },
        "openrouter/moonshotai/kimi-k2-thinking": {
          "alias": "thinking",
          "streaming": true
        },
        "openrouter/openrouter/auto": {
          "alias": "auto",
          "streaming": true
        }
      },
      "workspace": "/app",
      "userTimezone": "Asia/Kolkata",
      "memorySearch": {
        "store": {
          "path": "/data/memory/{agentId}.sqlite"
        }
      }
    },
    "list": [
      {
        "id": "anrakpilot",
        "name": "AnrakPilot",
        "default": true,
        "identity": {
          "name": "AnrakPilot",
          "emoji": "⚖️"
        }
      }
    ]
  },
  "models": {
    "mode": "replace",
    "providers": {
      "openrouter": {
        "baseUrl": "https://openrouter.ai/api/v1",
        "api": "openai-completions",
        "authHeader": true,
        "models": [
          {
            "id": "openrouter/free",
            "name": "Free (Auto-Routed Free Models)",
            "reasoning": false,
            "input": ["text"],
            "cost": {
              "input": 0,
              "output": 0,
              "cacheRead": 0,
              "cacheWrite": 0
            },
            "contextWindow": 200000,
            "maxTokens": 16384
          },
          {
            "id": "moonshotai/kimi-k2-thinking",
            "name": "Kimi K2 Thinking (Reasoning)",
            "reasoning": true,
            "input": ["text"],
            "cost": {
              "input": 0.4,
              "output": 1.75,
              "cacheRead": 0.1,
              "cacheWrite": 0.4
            },
            "contextWindow": 262144,
            "maxTokens": 16384
          },
          {
            "id": "openrouter/auto",
            "name": "Auto (Cost-Optimized Fallback)",
            "reasoning": false,
            "input": ["text"],
            "cost": {
              "input": 0.5,
              "output": 2.0,
              "cacheRead": 0.1,
              "cacheWrite": 0.5
            },
            "contextWindow": 200000,
            "maxTokens": 16384
          }
        ]
      }
    }
  },
  "plugins": {
    "slots": {}
  },
  "gateway": {
    "controlUi": {
      "enabled": false
    },
    "http": {
      "endpoints": {
        "chatCompletions": {
          "enabled": true
        }
      }
    }
  },
  "browser": {
    "enabled": true,
    "headless": true,
    "noSandbox": true,
    "defaultProfile": "openclaw"
  },
  "hooks": {
    "internal": {
      "enabled": true
    }
  },
  "commands": {
    "restart": false
  }
}
